 <div class="col l6 col m6 col s12">
                        <div class="card-container"> 
                        
                            <!-- Vertical News Box -->
                            <div class="news vertical ">
                            <div class="media-name">
                             <p onload="formatfeedurl('category.meta.title')">${category.meta.title}</p>
                            </div>
                                    <!-- News Image -->    
                                <div class="news-image">
                                        <img class="responsive-img" onerror="this.style.display='none';" src="${category.media_image}"  alt="news Image">
                                </div>
                                    <!-- News Description -->
                                <div class="news-description">
                                        <div class="news-time">
                                            <i class="fa fa-clock-o"></i> <time class="timeago" datetime="${category.pubDate[0]}"> </time>
                                          <!-- Your share button code -->
                            <div class="fb-share-button" 
                                data-href="http://nipashemedia.com/local?title=${category.title}" 
                                data-layout="button_count">
                            </div>
                                        </div>
                                        
                                        <div class="news-title" onclick="openSource('${category.linkid}','${category.title[0]}')" onchange="formatfeedurl('${category.title[0]}')"><h2><a>${category.title[0]}</a> </h2></div>
                                        <div class="news-content truncate" id="wrapper">${category.description[0].replace(/(<([^>]+)>)/ig,"")} </div>
                                </div>
                            </div>
                          
